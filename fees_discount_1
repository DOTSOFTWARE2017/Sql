create table fees_discount 
(
	fees_discount_id serial ,
	Student_admission_id  int not null,
	fee_discount_type varchar(20) not null,
	discount_amount int not null,
	fee_master_id int not null,
	student_id int not null,
	academic_year date,
	Created_Date date,
	Updated_By varchar(20),
	Updated_Date date,
	foreign key (Fee_master_id) references Fee_master(Fee_master_id)

)
insert into fees_discount
(
	Student_admission_id,
	fee_discount_type,
	discount_amount,
	fee_master_id,
	student_id,
	academic_year,
	Created_Date,
	Updated_By,
	Updated_Date
)

values
(
	1,
	'employee',
	1000,
	1,
	1,
	'2001-06-01',
	'2001-06-01',
	'kalaiyan',
	'2001-06-01'
);
